<% layout('layout/temp') -%>
<style>
    .card{
        border: 2px solid black;
        background-color: beige;
    }
</style>
<div class="row">
       <h3>Welcome @<b><%= currUser.username %></b> to your profile!</h3>
        <div class="col-8 offset-4">
          <div class="card" style="width: 30rem;">
            <img src="https://i.pinimg.com/736x/26/0e/83/260e8334d9f83815f88b6c741bdd613b.jpg" class="card-img-top" alt="...">
            <div class="card-body">
                <p class="card-text">
                    <ul>
                        <li>Name: <%= currUser.yourname %></li>
                        <li>Date Of Birth: <%= currUser.dob %></li>
                        <li>Email: <%= currUser.email %></li>
                        <li>Contact Number: <%= currUser.phone %></li>
                    </ul>
                </p>
            </div>
          </div>
        </div>
    </div>


